{
    "compilerOptions": {
        "target": "ES6",
        "module": "CommonJS",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "moduleResolution": "node",
        "noImplicitAny": false
    },
    "include": [
        "src/**/*",
        // "types"
    ],
    "exclude": [
        "node_modules"
    ]
}